<Project Sdk="Microsoft.NET.Sdk">
	<!--Common properties-->
	<PropertyGroup>
		<LangVersion>12.0</LangVersion>
		<AssemblyName>MMXOD</AssemblyName>
		<OutputType>WinExe</OutputType>
		<Nullable>disable</Nullable>
		<Version>29.02</Version>
		<AssemblyVersion>29.02</AssemblyVersion>
		<ApplicationIcon>app.ico</ApplicationIcon>
		<TargetFramework>net6.0-windows</TargetFramework>
		<ImplicitUsings>false</ImplicitUsings>
		<SelfContained>false</SelfContained>
		<OutputPath>./bin/</OutputPath>
		<!--OutDir>./bin/anycpu/</OutDir-->
		<RunWorkingDirectory>./bin/runenv/</RunWorkingDirectory>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
		<DebugType>embedded</DebugType>
		<Deterministic>true</Deterministic>
		<NeutralLanguage>en</NeutralLanguage>
		<FileVersion>20.0</FileVersion>
	</PropertyGroup>

	<!--For making output go to anycpu if the RuntimeIdentifier is empty-->
	<PropertyGroup Condition="'$(RuntimeIdentifier)' == ''">
		<OutputPath>./bin/anycpu/</OutputPath>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)' == 'Debug2'">
		<OutputPath>./bin/anycpu2/</OutputPath>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Watch2)' == 'true'">
		<OutputPath>./bin/anycpu2/</OutputPath>
	</PropertyGroup>

	<!--Dependencies-->
	<ItemGroup>
		<PackageReference Include="protobuf-net" Version="3.2.30" />
		<PackageReference Include="protobuf-net.Core" Version="3.2.30" />
		<PackageReference Include="SFML.Graphics" Version="2.5.1" />
		<PackageReference Include="SFML.Window" Version="2.5.1" />
		<PackageReference Include="SFML.System" Version="2.5.1" />
		<PackageReference Include="SFML.Audio" Version="2.5.1" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
		<PackageReference Include="DeviceId" Version="6.6.0" />
		<PackageReference Include="Profanity.Detector" Version="0.1.8" />
		<PackageReference Include="SpaceWizards.Lidgren.Network" Version="0.3.0" />
		<PackageReference Include="System.IO.Hashing" Version="8.0.0" />
	</ItemGroup>
</Project>
